{
  "expo": {
    "name": "Aquaware",
    "slug": "aquaware",
    "description":"Aquaware is an app for saving water.",
    "scheme": "aquaware",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/AquawareLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/BigLogo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSFaceIDUsageDescription": "This app uses FaceID to authenticate users.",
        "NSBiometricUsageDescription": "This app uses biometric authentication to secure access."
      },
      "bundleIdentifier": "com.anonymous.aquaware"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/AquawareLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.aquaware",
      "permissions": [
        "INTERNET",
        "ACCESS_FINE_LOCATION",
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "b79becc4-3630-4361-af85-d4455a64add2"
      }
    },
    "owner": "aquaware_bulgaria"
  }
}
